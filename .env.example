# VorteX WellMind Environment Configuration
# Copy this to .env and fill in your credentials

# IBM Watson AI Care (for chatbot responses)
AICARE_API_KEY=your_watson_orchestrate_api_key_here

# IBM watsonx.ai (backend text generation)
WATSONX_API_KEY=your_watsonx_api_key_here
WATSONX_URL=https://us-south.ml.cloud.ibm.com/ml/v1/text/generation?version=2023-05-29
WATSON_MODEL=ibm/granite-13b-chat-v2

# IBM watsonx Orchestrate (automation workflows)
ORCHESTRATE_API_KEY=your_orchestrate_api_key_here
ORCHESTRATE_URL=https://api.ibm.com/orchestrate/v1/workflows/burnout-followup

# Hedera Hashgraph (blockchain audit trail)
HEDERA_ACCOUNT_ID=0.0.YOUR_ACCOUNT_ID
HEDERA_PRIVATE_KEY=your_hedera_private_key_here
HEDERA_TOPIC_ID=0.0.YOUR_TOPIC_ID

# Email Notifications (optional - for automated wellness emails)
# Get this from Google Account > Security > 2-Step Verification > App Passwords
GMAIL_APP_PASSWORD=your_gmail_app_password_here
